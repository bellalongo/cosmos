    "### Query \n",
    "make $i_{mag} < 18$\n",
    "~~~\n",
    "    SELECT *\n",
    "    FROM mean_param\n",
    "    JOIN magnitudes ON mean_param.iau_name = magnitudes.iau_name\n",
    "    WHERE magnitudes.i < 18;\n",
    "~~~"
   ]
